{
  "name": "jonas-passwordless-nodejs",
  "version": "1.0.0",
  "description": "Passwordless Node.js library",
  "author": "Bitwarden",
  "contributors": [
    "Jonas Hendrickx"
  ],
  "keywords": [
    "bitwarden",
    "passwordless",
    "webauthn",
    "fido2"
  ],
  "scripts": {
