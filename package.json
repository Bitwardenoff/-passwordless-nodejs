  "license": "Apache-2.0",
  "dependencies": {
    "axios": "1.7.7"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/preset-env": "7.25.4",
    "@babel/preset-typescript": "7.24.7",
    "@jest/globals": "29.7.0",
    "@rollup/plugin-typescript": "12.1.0",
    "@types/node": "18.11.19",
    "@typescript-eslint/eslint-plugin": "8.7.0",
    "@typescript-eslint/parser": "8.7.0",
    "babel-jest": "29.7.0",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "npm-run-all2": "6.2.3",
    "rimraf": "5.0.10",
    "rollup": "4.22.5",
    "rollup-plugin-dts": "6.1.1",
    "ts-jest": "29.2.5",
    "typescript": "5.6.2"
  },
  "files": [
    "dist/types/passwordless-api.d.ts",
    "dist/cjs/passwordless-api.cjs",
    "dist/cjs/passwordless-api.cjs.map",
    "dist/esm/passwordless-api.mjs",
    "dist/esm/passwordless-api.mjs.map"
  ],
  "main": "dist/cjs/passwordless-api.cjs",
  "types": "dist/types/passwordless-api.d.ts",
  "engines": {
    "node": ">=18.0"
  }
